{
  "expo": {
    "name": "实用小词典",
    "description": "Extremely simple and functional mobile dictionary.",
    "slug": "jc-dictionary",
    "version": "0.2.2",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d4f6d5"
    },
    "primaryColor": "#d4f6d5",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cqcreative.jcdictionary"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#d4f6d5"
      },
      "package": "com.cqcreative.jcdictionary"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "savePhotosPermission": "允许简单粗暴实用小词典保存至相册"
        }
      ]
    ]
  }
}
