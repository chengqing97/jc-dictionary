    "expo-asset": "~8.5.0",
    "expo-av": "~11.2.3",
    "expo-dev-client": "~0.9.5",
    "expo-file-system": "~14.0.0",
    "expo-font": "~10.1.0",
    "expo-sqlite": "~10.2.0",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7",
    "react-native-zip-archive": "^6.0.8",
    "recoil": "^0.7.3-alpha.2"
  },
